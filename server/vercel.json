{
  "version": 2,
  "builds": [
    {
      "src": "main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "main.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
